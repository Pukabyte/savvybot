# Discord bot token from Discord Developer Portal
DISCORD_TOKEN=your_discord_token_here

# URL for your Ollama server
OLLAMA_SERVER_URL=http://localhost:11434

# Discord channel ID for forum monitoring
FORUM_CHANNEL_ID=your_channel_id_here

# URLs to scrape for knowledge base, separated by commas if multiple
DOCUMENTS_URLS=https://example.com/docs

# Ollama model to use for responses
OLLAMA_MODEL=ollama_model_name
